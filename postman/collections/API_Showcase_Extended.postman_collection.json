  "item": [
    {
      "name": "Payments",
      "item": [
        {
          "name": "Calculate Payment",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "Authorization",
                "value": "Bearer {{Token}}"
              }
            ],
            "url": {
              "raw": "{{API_BASE_URL}}/v1/payments/calculate",
              "host": [
                "{{API_BASE_URL}}"
              ],
              "path": [
                "v1",
                "payments",
                "calculate"
              ]
            },
            "body": {
              "mode": "raw",
              "raw": "{\n  \"Inst_Num\": \"{{Inst_Num}}\",\n  \"Application_Num\": \"{{Application_Num}}\",\n  \"Payload\": {}\n}"
            }
          },
          "event": [
            {
              "listen": "test",
              "script": {
                "type": "text/javascript",
                "exec": [
                  "pm.test('status is 2xx', () => pm.response.to.be.success);",
                  "pm.test('responds fast', () => pm.expect(pm.response.responseTime).below(5000));",
                  "pm.test('has JSON content-type', () => pm.response.to.have.header('content-type'));"
                ]
              }
            }
          ]
        }
      ]
    },
